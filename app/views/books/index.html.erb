<header>
    <h1>Books</h1>
</header>
<main>


    <tr>
    </tr>
    #上のテーブルタグに挿入
    <h4>Title</h4>
    <h4>Body</h4>

    <h2>New book</h2>

    <%= form_for(@book) do |f| %>

        <h4>Title</h4>
        <%= f.text_field :title %>

        <h4>Body</h4>
        <%= f.text_area :body %>

        <%= f.submit 'Create Book' %>

    <% end %>
</main>